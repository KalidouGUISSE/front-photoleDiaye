{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/front",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "server/server.mjs": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server/server.mjs"
    }
  ]
}